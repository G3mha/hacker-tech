{
  "manifest_version": 2,
  "name": "VANguard",
  "version": "1.0",
  "permissions": ["http://*/*", "https://*/*"],
  "browser_action": {
    "default_icon": "button/shield.png",
    "default_title": "VANguard",
    "default_popup": "pages/index.html"
  },
  "web_accessible_resources": [
    "pages/3rd-party.html",
    "pages/hijacking.html",
    "pages/storage.html",
    "pages/cookies.html",
    "pages/canvas.html"
  ]
}
